{
  "name": "seite",
  "version": "0.0.1",
  "description": "fastest way for writing stuff",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "sample": "node src/index.mjs",
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc -p conf/typescript/es.json --noEmit",
    "coverage": "jest --coverage",
    "build-commonjs": "tsc -p conf/typescript/commonjs.json",
    "build-es": "tsc -p conf/typescript/es.json",
    "build": "npm run build-commonjs && npm run build-es",
    "prepack": "npm run build"
  },
  "files": [
    "dist/es",
    "dist/commonjs"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loreanvictor/seite.git"
  },
  "keywords": [
    "docs",
    "blogs",
    "markdown",
    "documentation",
    "javascript"
  ],
  "author": "Eugene Ghanizadeh Khoub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loreanvictor/seite/issues"
  },
  "homepage": "https://github.com/loreanvictor/seite#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "github:sindresorhus/tsconfig",
    "babel-jest": "^29.4.1",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "^21.1.0"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "hastscript": "^7.2.0",
    "rehype-document": "^6.1.0",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.3",
    "rehype-wrap": "^1.1.0",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "vfile-matter": "^4.0.0",
    "vite": "^4.1.1"
  }
}
